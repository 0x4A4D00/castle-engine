function main()
  my_int := 0;
  my_vec2f := vector(44, 10);
  if( my_vec2f = vector(44, 10),

    my_float := vector_dot(my_vec2f, vector(666, 777));
    vector_set(my_vec2f, 0, vector_get(my_vec2f, my_int) + 456);
    { now my_vec2f should be (456 + 44, 10) }
    my_vec2f := my_vec2f + vector(0, 13);
    { now my_vec2f should be (456 + 44, 10 + 13) }
    vector_set(my_vec2f, vector_get_count(my_vec2f) - 1, vector_length(my_vec2f)),

    { else - should not happen } my_float := 666)

function main_3f()
  my_int := 0;
  my_vec3f := vector(44, 10, 33);
  if( my_vec3f = vector(44, 10, 33),

    my_float := vector_dot(my_vec3f, vector(666, 777, 91));
    vector_set(my_vec3f, 0, vector_get(my_vec3f, my_int) + 456);
    { now my_vec3f should be (456 + 44, 10, 33) }
    my_vec3f := my_vec3f + vector(0, 13, 0);
    { now my_vec3f should be (456 + 44, 10 + 13, 33) }
    vector_set(my_vec3f, vector_get_count(my_vec3f) - 1, vector_length(my_vec3f)),

    { else - should not happen } my_float := 666)
